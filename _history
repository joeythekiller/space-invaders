onPressed(Button.A, function () {\n    player.move(-1)\n})\n// Shoot bullet if none on screen\ninput.onButtonPressed(Button.AB, function () {\n    if (!(bulletInMotion)) {\n        bulletInMotion = true\n        bullet = game.createSprite(player.get(LedSpriteProperty.X), player.get(LedSpriteProperty.Y) - 1)\n        control.inBackground(function () {\n            while (bullet.get(LedSpriteProperty.Y) > 0) {\n                bullet.change(LedSpriteProperty.Y, -1)\n                basic.pause(100)\n            }\n            bullet.delete() // Remove bullet once off-screen\n            bulletInMotion = false\n        })\n    }\n})\ninput.onButtonPressed(Button.B, function () {\n    player.move(1)\n})\nlet player: game.LedSprite = null\n// Keeps track if the bullet is on screen\nlet bulletInMotion = false\n// Bullet (initially null)\nlet bullet: game.LedSprite = null\n// Player position\nplayer = game.createSprite(2, 4)\n// Enemy position\nlet enemy = game.createSprite(Math.randomRange(0, 4), 0)\n// Move the enemy down one step at a time\nbasic.forever(function () {\n    basic.pause(500)\n    enemy.change(LedSpriteProperty.Y, 1)\n    // Reset the enemy if it reaches the bottom\n    if (enemy.get(LedSpriteProperty.Y) == 4) {\n        enemy.set(LedSpriteProperty.X, Math.randomRange(0, 4))\n        enemy.set(LedSpriteProperty.Y, 0)\n    }\n    // Check if bullet hits the enemy\n    if (bullet && bullet.isTouching(enemy)) {\n        enemy.delete()\n        bullet.delete()\n        bulletInMotion = false\n        game.addScore(1)\n        enemy = game.createSprite(Math.randomRange(0, 4), 0)\n    }\n})\n","README.md":"","pxt.json":"{\n    \"name\": \"space invaders\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\"\n    ],\n    \"preferredEditor\": \"tsprj\"\n}\n"}},{"timestamp":172733811310